mode: user.german
-
settings():
	speech.engine = 'webspeech'
	speech.language = 'de_DE'

<user.prose>: auto_insert(prose)

# zeichen
neue Zeile: "\n"
Komma: ", "
Punkt: "."
Ausrufezeichen: "!"
Fragezeichen: "?"
Klammer auf: "("
Klammer zu: ")"
Leerzeichen: " "

# navigation
gehe zum Ende der Zeile: edit.line_end()
gehe zum Beginn der Zeile: edit.line_start()


